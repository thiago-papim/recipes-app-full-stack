  "devDependencies": {
    "nodemon": "^2.0.15",
    "@types/express": "^4.17.17",
    "@types/node": "^20.4.8",
    "@types/sequelize": "^4.28.15",
    "@types/validator": "^13.11.1",
    "express-async-errors": "^3.1.1",
    "ts-node": "^10.7.0",
    "ts-node-dev": "1.1.8"
  },
  "dependencies": {
    "mysql2": "^2.3.3",
    "express": "^4.17.1",
    "sequelize-cli": "6.2.0",
    "sequelize": "^6.3.4",
    "typescript": "4.4"
  }
}
